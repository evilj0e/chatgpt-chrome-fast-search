{
  "name": "ChatGPT Fast Search",
  "description": "Allows to make requests to OpenAI's ChatGPT directly from the search bar. Just type `gpt`, press `Tab` and your search request",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "index.js"
  },
  "omnibox": { "keyword": "gpt" },
  "action": {
    "default_icon": {
      "16": "/icons/16.png",
      "32": "/icons/32.png",
      "48": "/icons/48.png",
      "128": "/icons/128.png"
    }
  },
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  }
}
